{
    "name": "parsedown/twig",
    "description": "Official Twig Extension of Parsedown.",
    "keywords": [
        "parsedown",
        "twig"
    ],
    "license": "MIT",
    "homepage": "http://parsedown.org",
    "support": {
        "issues": "https://github.com/parsedown/twig/issues",
        "source": "https://github.com/parsedown/twig"
    },
    "authors": [
        {
            "name": "Eduardo Agostini",
            "email": "edu.agostini@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "erusev/parsedown": "~1.6"
    },
    "require-dev": {
        "orchestra/testbench": "^3.4",
        "php": ">=7.1",
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Parsedown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
